{
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [
    {
      "files": [
        "*.ts"
      ],
      "plugins": [
        "tsday"
      ],
      "extends": [
        "plugin:tsday/recommended"
      ],
      "rules": {
        "tsday/import-check": "error"
      }
    }
  ]
}